{
  "name": "newtelco-gatsby-netlify",
  "private": true,
  "description": "Gatsby example site using MDX and dependency-free i18n",
  "author": "LekoArts <hello@lekoarts.de>",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "npm run develop",
    "prettier": "prettier --write **/*.{md,css,scss,yaml,yml,ts}",
    "lint": "eslint src/**/*.{js,jsx}"
  },
  "dependencies": {
    "@palmabit/react-cookie-law": "^0.3.0",
    "framer-motion": "^1.11.0",
    "gatsby": "^2.22.0",
    "gatsby-image": "^2.4.4",
    "gatsby-plugin-env-variables": "^1.0.2",
    "gatsby-plugin-manifest": "^2.4.6",
    "gatsby-plugin-netlify-cms": "^4.3.2",
    "gatsby-plugin-offline": "^3.2.4",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.6",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-remark-images": "^3.3.5",
    "gatsby-remark-lazy-load": "^1.0.2",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-source-filesystem": "^2.3.5",
    "gatsby-transformer-json": "^2.4.2",
    "gatsby-transformer-remark": "^2.8.10",
    "gatsby-transformer-sharp": "^2.5.2",
    "isomorphic-unfetch": "^3.0.0",
    "lazysizes": "^5.1.2",
    "lodash.uniqby": "^4.7.0",
    "netlify-cms-app": "^2.12.12",
    "prismjs": "^1.20.0",
    "pure-react-carousel": "^1.27.1",
    "react": "^16.13.1",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^5.2.1",
    "react-intersection-observer": "^8.26.2",
    "react-loader-spinner": "^3.1.14",
    "react-notifications": "^1.6.0",
    "react-query": "^1.5.2",
    "react-select": "^3.1.0",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.0",
    "styled-icons": "^8.4.2",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "prettier": "^1.18.2",
    "standard": "^14.3.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "graphql": false
    },
    "rules": {
      "react/prop-types": [
        0
      ]
    },
    "parser": "babel-eslint"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "none",
    "singleQuote": true
  }
}
